{
  "name": "uilematisch",
  "version": "1.0.0",
  "description": "A discord bot I develop for myself when I'm bored!",
  "keywords": [
    "typescript",
    "discord",
    "discordjs",
    "discordbot",
    "discordtypescript"
  ],
  "bugs": {
    "url": "https://github.com/Manacchi/Uilematisch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Manacchi/Uilematisch"
  },
  "license": "ISC",
  "author": "Manacchi",
  "type": "module",
  "imports": {
    "#config": "./src/config.ts",
    "#helpers/*": "./src/helpers/*",
    "#interfaces": "./src/interfaces/index.ts",
    "#models": "./src/models/index.ts",
    "#src/*": "./src/*",
    "#structures/*": "./src/structures/*"
  },
  "exports": null,
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "lint": "eslint .",
    "start": "ts-node ./src/index.ts -r",
    "start:dist": "node ./dist/index.js -r",
    "sync": "ts-node ./src/dbInit.ts -a",
    "sync:dist": "node ./src/dbInit.ts -a"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.13.0",
    "@distube/soundcloud": "^1.2.2",
    "@distube/spotify": "^1.4.2",
    "@distube/yt-dlp": "^1.1.3",
    "canvas": "^2.10.2",
    "chalk": "^5.1.2",
    "common-tags": "^1.8.2",
    "concurrently": "^7.6.0",
    "discord.js": "^14.6.0",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "libsodium-wrappers": "^0.7.10",
    "node-fetch": "^3.3.0",
    "sequelize": "^6.26.0",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
